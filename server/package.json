{
  "name": "nestjs-prisma-boilerplate",
  "version": "0.0.1",
  "description": "NestJS Boilerplate with Prisma ORM",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset --force",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:deploy": "prisma migrate deploy",
    "deploy": "nest build && pm2 startOrRestart ecosystem.config.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.758.0",
    "@bull-board/api": "^6.13.0",
    "@bull-board/express": "^6.13.0",
    "@casl/ability": "^6.7.3",
    "@nestjs/bull": "^11.0.3",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^10.4.15",
    "@nestjs/event-emitter": "^2.1.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/platform-socket.io": "^10.4.15",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/websockets": "^10.4.15",
    "@nodeteam/nestjs-pipes": "^1.5.1",
    "@nodeteam/nestjs-prisma-pagination": "^1.0.6",
    "@prisma/client": "^6.4.0",
    "@ucast/sql": "^1.0.0-alpha.1",
    "bcrypt": "^5.1.1",
    "bull": "^4.16.5",
    "bullmq": "^5.60.0",
    "cache-manager": "^7.2.3",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "express-basic-auth": "^1.2.1",
    "ioredis": "^5.5.0",
    "luxon": "^3.5.0",
    "multer": "^1.4.5-lts.1",
    "nest-winston": "^1.10.2",
    "redis": "^5.8.3",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "socket.io-redis-adapter": "^8.4.6",
    "ua-parser-js": "^2.0.2",
    "uuid": "^11.0.5",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-papertrail-transport": "^1.0.9",
    "winston-slack-webhook-transport": "^2.3.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/luxon": "^3.4.2",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.17.19",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": { "version": "6.4.0", "seed": "ts-node prisma/seed.ts" },
    "tsx": "^4.19.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3"
  }
}
